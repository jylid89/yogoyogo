<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="activity">

<!-- 리스트 보는 sql -->
<select id="activitySelect" resultType="yogo.activity.dto.ActivityVO">
	SELECT * FROM TB_MARKETING
</select>

<!-- 글 쓰는 sql -->
<insert id="activityInsert" parameterType="yogo.activity.dto.ActivityVO">
	 INSERT INTO TB_MARKETING VALUES( MARK_SEQ.nextval, 'TRUCK003', #{mark_content}, to_char(sysdate, 'yyyy-mm-dd'), '0','0','0' )
</insert>


</mapper>



















